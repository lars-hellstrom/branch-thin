beginfig(1);
   metamer := 0.5cm;
   stem := 1cm;
   pen AO[], nodedot;
   AO1 := pencircle scaled 1bp;
   AO2 := pencircle scaled 0.5bp;
   path nodecircle; nodecircle := fullcircle scaled 2bp;
   pair Z[];
   
   cycles := 7;
   
   Z0 := cycles*metamer*(1,1) + (stem,stem);
   label(btex AO0 etex scaled 0.5, Z0);
   
   pickup AO2;
   draw fullcircle scaled stem shifted Z0;
   for angle := 0, 120, 240:
      Z1 := Z0 + 0.5stem * dir angle;
      draw Z1 -- (Z1 + (cycles-1)*metamer*dir angle) withpen AO1;
      label(btex AO1 etex scaled 0.5, Z1 + cycles*metamer*dir angle);
      for GC:= 1 upto cycles-1:
         fill nodecircle shifted Z1;
         for subangle := angle-60, angle+60:
            draw Z1 -- Z1 + (cycles-GC)*metamer*dir subangle;
            label(btex AO2 etex scaled 0.5, Z1 + (cycles-GC+1)*metamer*dir subangle);
            Z2 := Z1;
            for sGC := GC+1 upto cycles-1:
               Z2 := Z2 + metamer*dir subangle;
               fill nodecircle shifted Z2;
            endfor
         endfor
         Z1 := Z1 + metamer*dir angle;
      endfor
   endfor

endfig;

beginfig(2);
  metamer := 0.5cm;
  stem := 1cm;
  
  def recursive_branch(
     expr depth, last_point, my_radius, mydir, halfstep
   ) =
      for me := my_radius * dir mydir :
         draw last_point -- me;
         if depth > 0:
            for next_radius := 
               metamer+-+my_radius*sind(halfstep) + my_radius*cosd(halfstep)
            :
               recursive_branch(depth-1, me, next_radius, mydir+halfstep,
                  0.5halfstep)
               recursive_branch(depth-1, me, next_radius, mydir-halfstep,
                  0.5halfstep)
            endfor
         fi
      endfor
   enddef;
   
   
   pickup pencircle scaled 0.5bp;
   recursive_branch(8, (0,stem), stem+metamer, 90, 15);
   fill stem*dir 60 {(0,1) zscaled dir 60} .. (0,stem) .. 
     {(0,1) zscaled dir120}stem*dir 120 -- (0,0) -- cycle
     withcolor 0.6[black,white];
   draw (stem+9*metamer)*dir120 -- (0,0) -- (stem+9*metamer)*dir60
     withpen pencircle scaled 0.2bp dashed evenly;
   interim linecap:=butt;
   draw stem*dir 60 {(0,1) zscaled dir 60} .. (0,stem) .. 
     {(0,1) zscaled dir120}stem*dir 120
     withpen pencircle scaled 1bp;
endfig;


