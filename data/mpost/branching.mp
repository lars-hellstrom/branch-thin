vardef b_func(expr mu,beta,d,n) :=  
   if n < if mu > 2: 7 else: 10 fi: 
      min( mu**n, beta*((n+1)**d) )
   else: 
      beta*((n+1)**d) 
   fi
enddef;

vardef g_func (expr mu,beta,d , k,n) :=
  b_func(mu,beta,d,n) / b_func(mu,beta,d,n-k)
enddef;

vardef M_func(expr n) := (0.1n)**3 enddef;

ux:=1.5mm;
uy:=10cm;

def functionpath(suffix x)(expr a,b,c)(text func) := begingroup
   (ux*a, (for x:=a: func endfor)*uy)
   for x:=a+b step b until c: -- (ux*x, uy*(func)) endfor
endgroup enddef;


