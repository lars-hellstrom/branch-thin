vardef b_func(expr mu,beta,d,n) :=  
   if n < if mu > 2: 7 else: 10 fi: 
      min( mu**n, beta*((n+1)**d) )
   else: 
      beta*((n+1)**d) 
   fi
enddef;

vardef hex_factor(expr nmin,n) :=
   if n < nmin: 1 else:
      for y := (n/nmin) +-+ 1:
         if 3*y*y<=1: 1 - angle(1,y)/30 else: 0 fi
      endfor
   fi
enddef;

