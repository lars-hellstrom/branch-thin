beginfig(6);
  metamer := 0.2cm;
  stem := 1cm;
  
  def recursive_branch_C(
     expr last_point, extra, maxdir, mindir
   ) (expr stepshead)(text stepstail) =
      for me := last_point + stepshead*metamer * dir(0.5[maxdir,mindir]) :
      for tailsum :=
         0 for t = stepstail: +t endfor
      :
         draw last_point -- me;
         if tailsum > 0:
            for next_arc := 
               0.5(maxdir-mindir) + 
               angle (tailsum, (extra+stepshead)*sind 0.5(maxdir-mindir))
            :
               recursive_branch_C(me, 0,
                  0.5[maxdir,mindir]+next_arc, 0.5[maxdir,mindir],
                  stepstail)
               recursive_branch_C(me, 0,
                  0.5[maxdir,mindir], 0.5[maxdir,mindir]-next_arc,
                  stepstail)
            endfor
         fi
      endfor endfor
   enddef;
   
   
   pickup pencircle scaled 0.5bp;
   recursive_branch_C((0,stem), 5, 120, 60,
     9,5,2,1,1,1,0,0);
   fill stem*dir 60 {(0,1) zscaled dir 60} .. (0,stem) .. 
     {(0,1) zscaled dir120}stem*dir 120 -- (0,0) -- cycle
     withcolor 0.6[black,white];
   draw (stem+22*metamer)*dir120 -- (0,0) -- (stem+22*metamer)*dir60
     withpen pencircle scaled 0.2bp dashed evenly;
   interim linecap:=butt;
   draw stem*dir 60 {(0,1) zscaled dir 60} .. (0,stem) .. 
     {(0,1) zscaled dir120}stem*dir 120
     withpen pencircle scaled 1bp;
endfig;


